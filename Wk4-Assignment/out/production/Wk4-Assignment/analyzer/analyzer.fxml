<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="CENTER" spacing="30.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="analyzer.Controller">
   <children>
      <Button fx:id="button" mnemonicParsing="false" onAction="#analyzeGrades" text="Analyze Grades">
         <VBox.margin>
            <Insets top="30.0" />
         </VBox.margin>
      </Button>
      <ListView fx:id="listView">
         <VBox.margin>
            <Insets bottom="40.0" left="40.0" right="40.0" />
         </VBox.margin>
      </ListView>
   </children>
</VBox>
